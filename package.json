{
  "name": "vanilla-snake",
  "version": "1.1.0",
  "description": "A snake game with vanilla javascript",
  "main": "/src/main.js",
  "scripts": {
    "start": "webpack-dev-server --progress --color --config webpack/dev.js",
    "build": "webpack --config webpack/prod.js",
    "lint": "eslint src/ webpack/",
    "test": "jest ./src"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": "git+https://github.com/im6/vanilla-snake.git",
  "keywords": [
    "snake",
    "es6",
    "webpack"
  ],
  "author": "im6",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/im6/vanilla_snake/issues"
  },
  "homepage": "https://github.com/im6/vanilla_snake#readme",
  "private": false,
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "pre-commit": "^1.2.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
