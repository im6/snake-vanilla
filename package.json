{
  "name": "vanilla-snake",
  "version": "1.2.0",
  "description": "A snake game with vanilla javascript",
  "main": "/src/main.js",
  "scripts": {
    "dev": "webpack-dev-server --color --config webpack/dev.js",
    "build": "webpack --config webpack/prod.js",
    "lint": "eslint src/ webpack/",
    "test": "jest ./src --coverage"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": "git+https://github.com/im6/vanilla-snake.git",
  "keywords": [
    "snake",
    "es6",
    "webpack"
  ],
  "author": "im6",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/im6/vanilla_snake/issues"
  },
  "homepage": "https://github.com/im6/vanilla_snake#readme",
  "private": false,
  "dependencies": {
    "@babel/core": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "babel-loader": "^8.2.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.1",
    "node-sass": "^5.0.0",
    "pre-commit": "^1.2.2",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {},
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {}
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  }
}
